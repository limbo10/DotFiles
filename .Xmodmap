! Control Left
! keycode 37    |   keysym 0xffe3   |   Control_L

! Control Right
! keycode 105   |   keysym 0xffe4   |   Control_R

! Window Key
! keycode 133   |   keysym 0xffeb   |   mod4

! Menu Key
! keycode 135   |   keysym 0xff67   |   Menu

clear control
clear mod4

add control = Super_L Menu
add mod4 = Control_L
keycode 135 = Control_R
